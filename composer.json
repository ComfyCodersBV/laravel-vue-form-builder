{
    "name": "comfycoders/laravel-vue-form-builder",
    "description": "A VueJS/Inertia FormBuilder package for Laravel",
    "keywords": [
        "ComfyCoders BV",
        "laravel",
        "laravel-vue-form-builder"
    ],
    "homepage": "https://github.com/comfycodersbv/laravel-vue-form-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "ComfyCoders BV",
            "email": "development@comfycoders.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^11.0||^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "TranquilTools\\FormBuilder\\": "src/",
            "TranquilTools\\FormBuilder\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TranquilTools\\FormBuilder\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TranquilTools\\FormBuilder\\FormBuilderServiceProvider"
            ],
            "aliases": {
                "FormBuilder": "TranquilTools\\FormBuilder\\Facades\\FormBuilder"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}